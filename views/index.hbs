<nav class="navbar navbar-success bg-success mb-4">
    <a href="/" class="navbar-brand text-light">Coder-House</a>
</nav>
<div class="container">
    <form action="api/products" method="post">
        <input type="text" name="title" id="title" placeholder={{title}}>
        <input type="number" name="price" id="price" placeholder={{price}}>
        <input type="text" name="thumbnail" id="thumbnail" placeholder={{thumbnail}}>
        <button class="btn btn-success my-2 my-sm-0" type="submit">Enviar</button>
    </form>

    {{#if print}}
    <hr>
    <hr>
    <div class="card" style="width: 18rem;">

        <div class="card-body">
            {{newProduct}}
            {{inicio}}
        </div>
    </div>
    {{/if}}
</div>